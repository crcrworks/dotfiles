alt - 0x1B : yabai -m window --ratio rel:0.05
alt - 0x18 : yabai -m window --ratio rel:-0.05

alt - h : yabai -m window --focus west || yabai -m window --focus recent
alt - j : yabai -m window --focus south || yabai -m window --focus recent
alt - k : yabai -m window --focus north || yabai -m window --focus recent
alt - l : yabai -m window --focus east || yabai -m window --focus recent

alt + shift - h : yabai -m window --swap west
alt + shift - j : yabai -m window --swap south
alt + shift - k : yabai -m window --swap north
alt + shift - l : yabai -m window --swap east

alt + ctrl + shift - h : yabai -m window --warp west
alt + ctrl + shift - j : yabai -m window --warp south
alt + ctrl + shift - k : yabai -m window --warp north
alt + ctrl + shift - l : yabai -m window --warp east

alt - 1 : yabai -m space --focus 1
alt - 2 : yabai -m space --focus 2
alt - 3 : yabai -m space --focus 3
alt - 4 : yabai -m space --focus 4
alt - a : yabai -m space --focus 5
alt - s : yabai -m space --focus 6
alt - f : yabai -m space --focus 7
alt - w : yabai -m space --focus 8

alt + shift - 1 : yabai -m window --space 1
alt + shift - 2 : yabai -m window --space 2
alt + shift - 3 : yabai -m window --space 3
alt + shift - 4 : yabai -m window --space 4
alt + shift - a : yabai -m window --space 5
alt + shift - s : yabai -m window --space 6
alt + shift - f : yabai -m window --space 7
alt + shift - w : yabai -m window --space 8

alt - tab : yabai -m space --focus recent

alt - return : yabai -m window --toggle zoom-fullscreen
ctrl + shift - f : yabai -m window --toggle float
ctrl + shift - s : yabai -m window --toggle sticky
ctrl + shift - p : yabai -m window --toggle sticky --toggle pip
ctrl + shift - e : yabai -m window --toggle expose
