{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "agents": {
    "Sisyphus": {
      "model": "openai/gpt-5.2-codex",
      "permission": {
        "bash": {
          "*": "ask",
          "gh pr diff *": "allow",
          "gh pr view *": "allow",
          "cargo run *": "allow",
          "cargo test *": "allow",
          "ls": "allow"
        }
      }
    },
    "Sisyphus-Junior": {
      "model": "openai/gpt-5.2-codex",
      "permission": {
        "bash": {
          "*": "ask",
          "gh pr diff *": "allow",
          "gh pr view *": "allow",
          "cargo run *": "allow",
          "cargo test *": "allow",
          "ls": "allow"
        }
      }
    },
    "plan": {
      "model": "zai-coding-plan/glm-4.7"
    },
    "Prometheus (Planner)": {
      "model": "zai-coding-plan/glm-4.7"
    },
    "Metis (Plan Consultant)": {
      "model": "zai-coding-plan/glm-4.7"
    },
    "Momus (Plan Reviewer)": {
      "model": "zai-coding-plan/glm-4.7"
    },
    "orchestrator-sisyphus": {
      "model": "zai-coding-plan/glm-4.7"
    },
    "multimodal-looker": {
      "model": "google/gemini-3-flash-preview"
    }
  }
}
