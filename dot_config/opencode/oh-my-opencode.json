{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "categories": {
    "default": {
      "model": "zai-coding-plan/glm-4.7",
      "permission": {
        "bash": {
          "*": "ask",
          "gh pr diff *": "allow",
          "gh pr view *": "allow",
          "cargo run *": "allow",
          "cargo test *": "allow",
          "ls": "allow"
        }
      }
    }
  },
  "agents": {
    "Sisyphus": {
      "category": "default"
    },
    "plan": {
      "category": "default"
    },
    "oracle": {
      "category": "default"
    },
    "explore": {
      "category": "default"
    },
    "librarian": {
      "category": "default"
    },
    "build": {
      "category": "default"
    },
    "OpenCode-Builder": {
      "category": "default"
    },
    "Prometheus (Planner)": {
      "category": "default"
    },
    "Metis (Plan Consultant)": {
      "category": "default"
    },
    "Momus (Plan Reviewer)": {
      "category": "default"
    },
    "frontend-ui-ux-engineer": {
      "category": "default"
    },
    "document-writer": {
      "category": "default"
    },
    "multimodal-looker": {
      "model": "google/gemini-3-flash-preview"
    },
    "orchestrator-sisyphus": {
      "category": "default"
    }
  }
}
